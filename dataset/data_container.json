{
	"name": "data_container",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage_source_container",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(replace(dataset().filename, '.parquet', ''), '-', formatDateTime(utcnow(), 'dd-MM-yyy'), '.parquet')",
					"type": "Expression"
				},
				"fileSystem": "data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}